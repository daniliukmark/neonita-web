generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model NeonSign {
    id        Int      @id @default(autoincrement())
    name      String
    price     Decimal  @db.Decimal(10,2)
    image     String
    inStock   Boolean
    quantity  Int @db.UnsignedSmallInt

    @@index([name])
}